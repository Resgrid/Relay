
root = true  ; top-most EditorConfig file

; Windows-style newlines with a newline ending every file
[*]
end_of_line = crlf
insert_final_newline = true
tab_width = 4

[*.js]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

[*.cs]
indent_style = tab
indent_size = 4
trim_trailing_whitespace = true

[*.xml]
indent_style = tab

[*.pubxml]
indent_style = tab

